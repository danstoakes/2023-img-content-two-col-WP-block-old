{
    "$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "danstoakes/img-content-two-col",
	"title": "Image Beside Content",
	"icon": "universal-access-alt",
	"category": "media",
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": ".content"
		},
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h2"
		},
		"mediaID": {
			"type": "number"
		},
		"mediaURL": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		}
	},
	"example": {},
	"editorScript": "file:./block.js",
	"style": "file:./style.css"
}